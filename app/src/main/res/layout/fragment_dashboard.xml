<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:nestedScrollingEnabled="true"
    android:background="#60FFFFFF">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout

            android:id="@+id/linear"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:orientation="horizontal"
            android:weightSum="1">


            <LinearLayout
                android:id="@+id/messageLinear"
                android:layout_width="200dp"
                android:layout_height="170dp"
                android:layout_gravity="center"
                android:layout_margin="10dp"
                android:layout_weight=".5"
                android:background="@drawable/viewstyledashboardleft"
                android:gravity="center"
                android:orientation="vertical">


                <ImageView
                    android:id="@+id/premium"
                    android:layout_width="70dp"
                    android:layout_height="40dp"
                    android:layout_below="@+id/txtOwner"
                    android:layout_gravity="left|center"
                    android:gravity="left|center"
                    android:src="@drawable/premium" />

                <ImageView
                    android:id="@+id/imgMsg"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_below="@+id/txtOwner"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:src="@drawable/quran" />

                <TextView
                    android:id="@+id/txtOwnerName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imgOwner"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:gravity="center"
                    android:text="Quran Reading"
                    android:textColor="#000
"
                    android:textSize="16sp"
                    android:textStyle="bold" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/agreementLinear"
                android:layout_width="200dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                android:layout_toRightOf="@+id/messageLinear"
                android:layout_weight=".5"
                android:background="@drawable/viewstyledashboardright"
                android:gravity="center"
                android:orientation="vertical">


                <ImageView
                    android:id="@+id/premium1"
                    android:layout_width="70dp"
                    android:layout_height="40dp"
                    android:layout_below="@+id/txtOwner"
                    android:layout_gravity="left|center"
                    android:gravity="left|center"
                    android:src="@drawable/premium" />

                <ImageView
                    android:id="@+id/imgAgree"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_below="@+id/txtTenant"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:src="@drawable/basic" />

                <TextView
                    android:id="@+id/txtTenantName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imgTenant"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:gravity="center"
                    android:text="Basic Quida"
                    android:textColor="#000"
                    android:textSize="16sp"
                    android:textStyle="bold" />

            </LinearLayout>

        </LinearLayout>


        <LinearLayout

            android:id="@+id/linear1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linear"
            android:layout_marginTop="30dp"
            android:orientation="horizontal"
            android:weightSum="1">


            <LinearLayout
                android:id="@+id/linearArabic"
                android:layout_width="200dp"
                android:layout_height="170dp"
                android:layout_gravity="center"
                android:layout_margin="10dp"
                android:layout_weight=".5"
                android:background="@drawable/viewstyledashboardleft"
                android:gravity="center"
                android:orientation="vertical">


                <ImageView
                    android:id="@+id/premiumArabic"
                    android:layout_width="70dp"
                    android:layout_height="40dp"
                    android:layout_below="@+id/txtOwner"
                    android:layout_gravity="left|center"
                    android:gravity="left|center"
                    android:src="@drawable/premium" />

                <ImageView
                    android:id="@+id/imgArabic"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_below="@+id/txtOwner"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:src="@drawable/arabicimg" />

                <TextView
                    android:id="@+id/txtArabic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imgOwner"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:gravity="center"
                    android:text="Arabic"
                    android:textColor="#000
"
                    android:textSize="16sp"
                    android:textStyle="bold" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/LinearUrdu"
                android:layout_width="200dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                android:layout_toRightOf="@+id/messageLinear"
                android:layout_weight=".5"
                android:background="@drawable/viewstyledashboardright"
                android:gravity="center"
                android:orientation="vertical">


                <ImageView
                    android:id="@+id/premiumUrdu"
                    android:layout_width="70dp"
                    android:layout_height="40dp"
                    android:layout_below="@+id/txtOwner"
                    android:layout_gravity="left|center"
                    android:gravity="left|center"
                    android:src="@drawable/premium" />

                <ImageView
                    android:id="@+id/imgUrdu"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_below="@+id/txtTenant"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:src="@drawable/urduimg" />

                <TextView
                    android:id="@+id/txtUrdu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imgTenant"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:gravity="center"
                    android:text="Urdu"
                    android:textColor="#000"
                    android:textSize="16sp"
                    android:textStyle="bold" />

            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:id="@+id/linear2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linear1"
            android:orientation="horizontal"
            android:weightSum="1">


            <LinearLayout
                android:id="@+id/serviceLinear"
                android:layout_width="200dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                android:layout_weight=".5"
                android:background="@drawable/viewstyledashboardleft"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imgService"
                    android:layout_width="120dp"
                    android:layout_height="90dp"
                    android:layout_below="@+id/txtOwner"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:src="@drawable/salahimg" />

                <TextView
                    android:id="@+id/txtService"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imgOwner"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:gravity="center"
                    android:text="Salah"
                    android:textColor="#000 "
                    android:textSize="16sp"
                    android:textStyle="bold" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/feedbackLinear"
                android:layout_width="200dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                android:layout_toRightOf="@+id/messageLinear"
                android:layout_weight=".5"
                android:background="@drawable/viewstyledashboardright"
                android:gravity="center"
                android:orientation="vertical">


                <ImageView
                    android:id="@+id/imgFeedback"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_below="@+id/txtTenant"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:src="@drawable/zkr" />

                <TextView
                    android:id="@+id/txtFeedback"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imgTenant"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:gravity="center"
                    android:text="Zikar"

                    android:textColor="#000"
                    android:textSize="16sp"
                    android:textStyle="bold" />

            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:id="@+id/linear4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linear2"
            android:gravity="center"
            android:orientation="horizontal"
            android:weightSum="1">

            <LinearLayout
                android:id="@+id/passwordLinear"
                android:layout_width="200dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                android:layout_weight=".5"
                android:background="@drawable/viewstyledashboardleft"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imgPass"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_below="@+id/txtOwner"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:src="@drawable/aa" />

                <TextView
                    android:id="@+id/txtPass"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imgOwner"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:gravity="center"
                    android:text="Adult Awareness"
                    android:textColor="#000 "
                    android:textSize="16sp"
                    android:textStyle="bold" />

            </LinearLayout>


            <LinearLayout

                android:id="@+id/subadminLinear"
                android:layout_width="200dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                android:layout_toRightOf="@+id/messageLinear"
                android:layout_weight=".5"
                android:background="@drawable/viewstyledashboardright"
                android:gravity="center"
                android:orientation="vertical">


                <ImageView
                    android:id="@+id/imgSubadmin"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_below="@+id/txtTenant"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:src="@drawable/qaaa" />

                <TextView
                    android:id="@+id/txtSubadmin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imgTenant"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:gravity="center"
                    android:text="Q &amp; A"
                    android:textColor="#000"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    tools:ignore="HardcodedText" />

            </LinearLayout>

        </LinearLayout>
    </RelativeLayout>
</ScrollView>
